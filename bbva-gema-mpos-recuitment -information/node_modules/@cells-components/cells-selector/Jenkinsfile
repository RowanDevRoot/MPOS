#!groovy

@Library("workflowlibs@master") _

pipeline {

    agent any
    parameters {
        file(description: 'Ether Configuration', name: 'etherConfig')
    }
    stages {
        stage('Checkout Global Library') {
            steps {
                script{
                    globalBootstrap {
                        libraryName   = "cellsworkflowlibs"
                        libraryBranch = "develop"

                        entrypointParams = [
                            type                  : "cellsComponent",
                            lint                  :  false,
                            sonarQube             :  false,
                            test                  :  false,
                            publish               :  true,
                            deploySecGCP          :  true
                        ]
                    }
                }
            }
        }
    }
}
